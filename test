#include <stdio.h>

int main()
{
    int j;
	for ( j = 0; j < 3; ++j )
	{
		if ( j == 2 )
		{
			print( "j\n" );
            break;
		}
	}

	for ( int x = 0; x < 3; ++x )
	{
		for ( int y = 0; y < 3; ++y )
		{
			for ( int z = 0; z < 3; ++z )
			{

				print( "xyz 27 times\n" );
			}
		}
	}

	int i = 1;
	while ( i < 10 )
	{
		print( "i\n" );
		i += 1;
	}
}
